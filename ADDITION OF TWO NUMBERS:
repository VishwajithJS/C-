#include <iostream>
using namespace std;

class Addition {
#include <iostream>
using namespace std;

class Addition {
public:
    int num1;
    int num2;
    int num3;

    void check() {
        num3 = num1 + num2; 
        cout << num3 << " is the sum" << endl;
    }
};

int main() {
    Addition num1;
    cout << "Enter 1st numbers:";
    cin >> num1.num1;
    cout << "Enter 2nd numbers:";
    cin>> num1.num2;
    num1.check();
    return 0;
}
